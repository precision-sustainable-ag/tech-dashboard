{
    "globalHeaders": {
        "content-security-policy": "object-src 'none'"
    },
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/img/*.{png,jpg,gif,webp}",
            "/css/*"
        ]
    },
    "mimeTypes": {
        "custom": "text/html"
    }
}